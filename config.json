			}
		]
	},
	"result": {
		"title": "result",
		"body_color": "black",
		"ratio": "2/1",
		"elements": [
			{
				"type": "image",
				"content": "/static/images/Abstract_2-Overlay_logo.png",
				"style": "width: 100%; height: 100%; object-fit: cover;"
			},
			{
				"type": "video",
				"content": "/static/videos/Galaxy Brain meme.webm",
				"style": "width: 100%; height: 100%; object-fit: cover;",
