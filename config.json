				"loop": true,
				"muted": true
			},
			{
				"type": "image",
				"content": "/static/images/home_icon.png",
				"style": "width: 10%; top: 5%; right: 5%;",
				"link": "/"
			},
			{
				"type": "input",
				"content": "your number",
				"style": "top: 50%; left: 45%;",
