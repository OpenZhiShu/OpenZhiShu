				"autoplay": true,
				"muted": true
			},
			{
				"type": "variable",
				"content": "result",
				"style": "top: 20%; left: 10%; font-size: 5em; background: dimgray; color: lightgray;",
				"appear": 3600
			},
			{
				"type": "image",
				"content": "/static/images/home_icon.png",
				"style": "width: 10%; top: 5%; right: 5%;",
				"link": "/",
