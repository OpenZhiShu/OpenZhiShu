{
	"homepage": {
		"title": "homepage",
		"body_color": "black",
		"ratio": "2/1",
		"elements": [
			{
				"type": "image",
				"content": "https://cdn.akamai.steamstatic.com/apps/csgo/images/csgo_react/cs2/header_bg.svg",
				"style": "width: 100%; height: 100%; object-fit: cover;"
			},
			{
				"type": "image",
				"content": "https://cdn.akamai.steamstatic.com/apps/csgo/images/csgo_react//cs2/header_ctt.png",
				"style": "height: 75%; bottom: 0%; left: 0%"
			},
			{
				"type": "image",
				"content": "/static/images/settings_icon.svg",
				"style": "width: 5%; top: 5%; right: 5%;",
				"link": "/settings"
			},
			{
				"type": "text",
				"content": "START",
				"style": "top: 50%; left: 70%; transform: translate(-50%, -50%); font-size: 5em; background: dimgray; color: lightgray;",
				"link": "/drawing"
			}
		]
	},
	"drawing": {
		"title": "drawing",
		"body_color": "black",
		"ratio": "2/1",
		"elements": [
			{
				"type": "image",
				"content": "https://cdn.akamai.steamstatic.com/apps/csgo/images/csgo_react/cs2/header_bg.svg",
				"style": "width: 100%; height: 100%; object-fit: cover;"
			},
			{
				"type": "image",
				"content": "/static/images/home_icon.svg",
				"style": "width: 5%; top: 5%; right: 5%;",
				"link": "/"
			},
			{
				"type": "input",
				"content": "your number",
				"style": "top: 50%; left: 45%;",
				"input_type": "number"
			},
			{
				"type": "jump",
				"content": "DRAW!!!",
				"style": "top: 60%; left: 45%;",
				"target": "element-2",
				"url_prefix": "/result/"
			}
		]
	},
	"result": {
		"title": "result",
		"body_color": "black",
		"ratio": "16/9",
		"elements": [
			{
				"type": "image",
				"content": "/static/images/result.png",
				"style": "width: 100%; height: 100%; object-fit: cover;",
				"appear": 1000
			},
			{
				"type": "video",
				"content": "/static/videos/unboxing.mp4",
				"style": "width: 100%; height: 100%; object-fit: cover;",
				"autoplay": true,
				"muted": true
			},
			{
				"type": "variable",
				"content": "result",
				"style": "top: 20%; left: 10%; font-size: 5em; background: dimgray; color: lightgray;",
				"appear": 8500
			},
			{
				"type": "image",
				"content": "/static/images/home_icon.svg",
				"style": "width: 5%; top: 5%; right: 5%;",
				"link": "/",
				"appear": 8500
			}
		]
	}
}
